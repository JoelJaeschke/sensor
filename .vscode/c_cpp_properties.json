{
    "configurations": [
        {
            "name": "ESP32-Win",
            "includePath": [
                "${workspaceFolder}/main/*",
                "${workspaceFolder}/build/*",
                "${workspaceFolder}/build/config/*",
                "${env:IDF_PATH}/components/**",
                "${env:IDF_TOOLS_PATH}/tools/**"
            ],
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/main/*",
                    "${workspaceFolder}/build/*",
                    "${workspaceFolder}/build/config/*",
                    "${env:IDF_PATH}/components/**",
                    "${env:IDF_TOOLS_PATH}/tools/**"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${workspaceFolder}/.vscode/VC_DATABASE.db"
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "${env:IDF_TOOLS_PATH}/tools/xtensa-esp32-elf/esp-2020r2-8.2.0/xtensa-esp32-elf/bin/xtensa-esp32-elf-g++.exe"
        }
    ],
    "version": 4
}